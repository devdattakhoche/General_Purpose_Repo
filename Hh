-- Check if table exists and create if it doesn't
IF NOT EXISTS (SELECT 1 FROM sysobjects WHERE name = 'YourTableName' AND type = 'U')
BEGIN
    CREATE TABLE YourTableName (
        jpm_post_trade_id VARCHAR(255), -- Adjust data type and length as needed
        ric VARCHAR(255)               -- Adjust data type and length as needed
    )
END

-- Insert data from temporary table #jpmPostTradeID2RICMapping
INSERT INTO YourTableName (jpm_post_trade_id, ric)
SELECT jpm_post_trade_id, ric
FROM #jpmPostTradeID2RICMapping
