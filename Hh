
DELIMITER //
CREATE PROCEDURE cursor_example()
BEGIN
    DECLARE done INT DEFAULT 0;
    DECLARE id INT;
    DECLARE cursor_example CURSOR FOR SELECT id FROM your_table;
    DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = 1;

    OPEN cursor_example;

    WHILE done = 0 DO
        FETCH cursor_example INTO id;
        IF NOT done THEN
            -- do something with the id variable
            INSERT INTO another_table (id) VALUES (id);
        END IF;
    END WHILE;

    CLOSE cursor_example;
END//
DELIMITER ;
